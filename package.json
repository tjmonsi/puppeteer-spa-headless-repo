{
  "name": "puppeteer-spa-headless-repo",
  "version": "0.0.1",
  "description": "This is the mini-repo for the bug on puppeteer",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/npm-run-all --parallel serve webpack-dev",
    "serve": "node ./src/server.js",
    "webpack-dev": "./node_modules/.bin/webpack --mode=development --watch",
    "webpack-once": "./node_modules/.bin/webpack --mode=development",
    "start": "npm run dev",
    "test-e2e": "./node_modules/.bin/jest",
    "test": "./node_modules/.bin/npm-run-all --parallel serve webpack-once test-e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmonsi/puppeteer-spa-headless-repo.git"
  },
  "author": "TJ Monserrat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmonsi/puppeteer-spa-headless-repo/issues"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [
      "beforeAll",
      "afterAll",
      "jest",
      "describe",
      "it"
    ]
  },
  "homepage": "https://github.com/tjmonsi/puppeteer-spa-headless-repo#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.9.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "must": "^0.13.4",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^1.10.0",
    "semistandard": "^13.0.1",
    "superstatic": "^6.0.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@littleq/core-lite": "0.0.5",
    "@littleq/element-lite": "^0.3.8",
    "firebase": "^5.5.8"
  }
